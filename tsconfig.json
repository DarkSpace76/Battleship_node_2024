{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ES2020",
        "baseUrl": "./",
        "outDir": "./dist",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noImplicitAny": false,
        "strictNullChecks": false,
        "skipLibCheck": true,
        "moduleResolution": "node",
    }
}